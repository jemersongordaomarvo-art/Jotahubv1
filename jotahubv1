local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextBox = Instance.new("TextBox")
local TextButton = Instance.new("TextButton")
 
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
 
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.402528971, 0, 0.365553916, 0)
Frame.Size = UDim2.new(0, 185, 0, 130)
Frame.Active = true
Frame.Draggable = true
 
TextBox.Parent = Frame
TextBox.BackgroundColor3 = Color3.fromRGB(52, 52, 52)
TextBox.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextBox.BorderSizePixel = 0
TextBox.Position = UDim2.new(0.135135129, 0, 0.292307705, 0)
TextBox.Size = UDim2.new(0, 134, 0, 21)
TextBox.Font = Enum.Font.SourceSans
TextBox.PlaceholderColor3 = Color3.fromRGB(178, 178, 178)
TextBox.PlaceholderText = "hitbox size here"
TextBox.Text = ""
TextBox.TextColor3 = Color3.fromRGB(178, 178, 178)
TextBox.TextSize = 14.000
 
TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(52, 52, 52)
TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
TextButton.BorderSizePixel = 0
TextButton.Position = UDim2.new(0.135135129, 0, 0.569230795, 0)
TextButton.Size = UDim2.new(0, 134, 0, 18)
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = "change"
TextButton.TextColor3 = Color3.fromRGB(178, 178, 178)
TextButton.TextSize = 14.000
 
local function CEIID_fake_script()
local script = Instance.new('LocalScript', TextButton)
 
local tb = script.Parent.Parent.TextBox
 
script.Parent.MouseButton1Click:Connect(function()
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
 
local function createSurroundingPart()
if workspace:FindFirstChild("hitbox") then
workspace.hitbox.Size = Vector3.new(tb.Text, tb.Text, tb.Text)
else
local surroundingPart = Instance.new("Part")
surroundingPart.Name = "hitbox"
surroundingPart.Size = Vector3.new(tb.Text, tb.Text, tb.Text)
surroundingPart.Position = character.PrimaryPart.Position
surroundingPart.Anchored = true
surroundingPart.CanCollide = false
surroundingPart.Transparency = 0.5
surroundingPart.BrickColor = BrickColor.new("Bright blue")
surroundingPart.Parent = workspace
surroundingPart.Touched:Connect(function(hit)
if hit.Name == "SoccerBall" then
local offset = character.HumanoidRootPart.CFrame.LookVector * 2
local newPosition = character.HumanoidRootPart.Position + offset
hit.CFrame = CFrame.new(newPosition, newPosition + character.HumanoidRootPart.CFrame.LookVector)
end
end)
 
game:GetService("RunService").Heartbeat:Connect(function()
surroundingPart.Position = character.PrimaryPart.Position
end)
end
end
 
createSurroundingPart()
end)
end
coroutine.wrap(CEIID_fake_script)()
